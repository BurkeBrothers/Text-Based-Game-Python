weapons = ["axe", "sword", "spear"]

#debugging purpose
#print(weapons)

inventory = []

def weapon_choice():
    print("These are your available weapons:")
    for i in range(len(weapons)):
        time.sleep(.5)
        print(f"{i}. {weapons[i]}")
    choice = int(input("What weapon do you want? "))
    inventory.append(weapons[choice])

weapon_choice()

